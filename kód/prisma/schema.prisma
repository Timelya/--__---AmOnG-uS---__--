generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id       Int      @id @default(autoincrement())
  name     String
  password String
  created  DateTime @default(now())
}

model event {
  id    Int       @id @default(autoincrement())
  name  String    @db.VarChar(255)
  start DateTime? @db.Timestamp(0)
  end   DateTime? @db.Timestamp(0)
}
